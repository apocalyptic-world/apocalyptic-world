:: Widget newgirl [widget]
<<widget newgirl>>
	<<include 'Girl names'>>
	<<set _gender = 0>>	
	<<include 'newnpc'>>

	<<set
		_npc.breasts = either('small', 'medium', 'big'),
		_npc.virgin = false,
		_npc.orgasms = 0,
		_npc.clothes = {
			top: either(1,2,3),
			bottom: either(1,2,3)
		}
	>>

	<<if (setup.getAge(_npc) <= 20 && setup.percentageChance(50))>>
		<<set 
			_npc.virgin = true,
			_npc.guys = 0,
			_npc.girls = 0,
			_npc.tguys = 0,
			_npc.tgirls = 0,
			_npc.anal = 0,
			_npc.bj = 0,
			_npc.dp = 0,
			_npc.pussy = 0
		>>
	<</if>>

	<<if setup.percentageChance(50)>>
		<<set _npcTraits = 1>>
		<<if setup.percentageChance(30)>>
			<<set _npcTraits = 2>>
		<</if>>
		<<set _npc.traits = setup.getRandomTraits(_npcTraits)>>
	<</if>>

	<<set $tmpGirl = _npc>>
<</widget>>


