// Setting up a volume control for the settings property 'masterVolume' w/ callback
Setting.addToggle("bgVolumeEnabled", {
	label : "Background sound enabled",
	default: true,
	onChange : function () {
		if (!settings.bgVolumeEnabled) {
			SimpleAudio.stop();
		}
	}
});

Setting.addRange("bgVolume", {
	label    : "Background volume",
	min      : 0,
	max      : 10,
	step     : 1,
	default  : 1,
	onChange : function () {
		const newVolume = settings.bgVolume / 20;
		SimpleAudio.volume(newVolume);
	}
});


