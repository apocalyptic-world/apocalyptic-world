:: Story JavaScript [script]
Config.passages.nobr = true;
Config.saves.autosave = "autosave";
Config.history.maxStates = 3;
Config.macros.ifAssignmentError = false;

//Settings
Setting.addToggle("autoplay", {
	label : "Autoplay video",
	default: true
});

Setting.addToggle("transEnabled", {
	label : "Trans content enabled",
	default: true
});

Setting.addToggle("gaysEnabled", {
	label : "Gay content enabled",
	default: true
});

Setting.addToggle("lesbiansEnabled", {
	label : "Lesbian content enabled",
	default: true
});

Setting.addList("sidebarBackground", {
    label   : "Sidebar background color",
    list    : ["yellow", "gray"],
    default : "yellow"
});

Setting.addToggle("fastTravelEnabled", {
	label : "Fast travel enabled",
	default: true
});

Config.navigation.override = pas => {
  if (!Story.has(pas)) return 'Wood cabin';
};

